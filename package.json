  "repository": {
    "type": "git",
    "url": "https://github.com/sellside/strings.git"
  },
  "author": {
    "name": "Brian Woodward",
    "url": "http://github.com/doowb"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/sellside/strings/blob/master/LICENSE-MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/sellside/strings/issues"
  },
  "dependencies": {
    "lodash": "~2.4.1",
    "frep": "~0.1.2",
    "underscore.string": "~2.3.3",
    "moment": "~2.5.0"
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "mocha": "~1.15.1",
    "blanket": "~1.1.5",
    "travis-cov": "~0.2.4",
    "meld": "~1.3.0"
  },
  "keywords": [
