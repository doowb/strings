{
  "name": "strings",
  "version": "0.2.0",
  "description": "String replacement and transformation library.",
  "main": "index.js",
  "scripts": {
    "test": "mocha && mocha -r blanket -R html-cov > test\\coverage\\coverage.html && mocha -r blanket -R json-cov > test\\coverage\\coverage.json && mocha -r blanket -R travis-cov",
    "travis-cov": {
      "threshold": 70
    },
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
